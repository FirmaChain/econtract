  "scripts": {
    "preinstall": "sh ./pre-install-yarn.sh",
    "postinstall": "sh ./post-install-yarn.sh",
    "start": "react-native start",
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "web": "webpack-dev-server --content-base app/web/public --hot --watch --history-api-fallback --useLocalIp --host 0.0.0.0",
    "desktop": "electron ./app/desktop/bin/main.js",
    "codepush:android": "appcenter codepush release-react -a FirmaChain/PoC-Android -d Production",
    "codepush:ios": "appcenter codepush release-react -a FirmaChain/PoC-iOS -d Production --plist-file ./ios/proj/Info.plist",
    "build:desktop": "electron-packager ./app/desktop/bin --overwrite",
    "android:debug:gms:keystore": "keytool -list -v -keystore ~/.android/debug.keystore -alias androiddebugkey -storepass android -keypass android",
    "android:i": "adb shell input keyevent 82"
  },
  "rnpm": {
    "assets": [
      "./app/mobile/fonts/",
      "./app/mobile/fonts/NotoSans/"
    ]
  },
  "jest": {
    "preset": "react-native",
    "setupFiles": [
      "./node_modules/appcenter/test/AppCenterMock.js",
      "./node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "./node_modules/appcenter-crashes/test/AppCenterCrashesMock.js",
      "<rootDir>/node_modules/appcenter/test/AppCenterMock.js",
      "<rootDir>/node_modules/appcenter-analytics/test/AppCenterAnalyticsMock.js",
      "<rootDir>/node_modules/appcenter-crashes/test/AppCenterCrashesMock.js"
    ]
  },
