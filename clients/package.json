{
  "name": "poc-prototype",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "babel-cli": "6.26.0",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-react-native-stage-0": "^1.0.1",
    "jest": "^22.4.4",
